{
  "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/index.csr.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "public",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "html",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/index.html"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "app-config",
      "urls": ["/assets/app-settings/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1,
        "maxAge": "0s",
        "timeout": "3s"
      }
    }
  ]
}
